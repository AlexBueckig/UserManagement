  "dependencies": {
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "chokidar": "^1.6.1",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.12.6",
    "lodash": "^4.16.6",
    "lodash.isnull": "^3.0.0",
    "nodemon": "^1.11.0",
    "pg": "^6.1.0",
    "validator": "^6.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  }
